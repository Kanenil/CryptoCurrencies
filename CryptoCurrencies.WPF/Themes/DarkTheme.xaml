    <SolidColorBrush x:Key="SecundaryBackgroundColor" Color="#161616"/>
    
    <SolidColorBrush x:Key="PrimaryTextColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="SecundaryTextColor" Color="#797878"/>

    <SolidColorBrush x:Key="ActiveColor" Color="#8dde3c"/>
